{
  "name": "acmcsuf.com",
  "version": "4.0.0",
  "private": true,
  "homepage": "https://acmcsuf.com/",
  "author": {
    "name": "EthanThatOneKid",
    "url": "https://etok.codes/"
  },
  "repository": {
    "url": "https://github.com/EthanThatOneKid/acmcsuf.com.git/"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:size": "npm i && size-limit",
    "test:coverage": "vitest run --coverage",
    "test:browser": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --compiler-warnings \"css-unused-selector:ignore,unused-export-let:error\"",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "host": "npm run all && npm run preview",
    "all": "npm run lint && npm run format && npm run check && npm run build && npm t"
  },
  "devDependencies": {
    "@playwright/test": "^1.34.3",
    "@size-limit/file": "^8.2.4",
    "@sveltejs/adapter-netlify": "next",
    "@sveltejs/kit": "^1.20.0",
    "@types/html-to-text": "^9.0.1",
    "@types/qrcode": "^1.5.0",
    "@types/rss": "^0.0.30",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitest/coverage-c8": "^0.31.3",
    "devalue": "^4.3.2",
    "dotenv": "^16.1.3",
    "esbuild": "^0.17.19",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.62.1",
    "size-limit": "^8.2.4",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4",
    "vitest": "^0.31.3"
  },
  "size-limit": [
    {
      "path": "node_modules",
      "limit": "256 MB"
    }
  ],
  "type": "module",
  "dependencies": {
    "@googleapis/calendar": "^6.0.0",
    "@js-temporal/polyfill": "^0.4.4",
    "html-to-text": "^9.0.5",
    "pomo": "github:ethanthatonekid/pomo#npm",
    "qrcode": "^1.5.3",
    "rrule": "^2.7.2",
    "rss": "^1.2.2",
    "svelte-confetti": "^1.2.2",
    "svelte-meta-tags": "^2.7.2"
  }
}
